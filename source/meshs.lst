    104 !------------------------------------------------------------------------
    105 !  Precompute the mesh stuff
    106 !------------------------------------------------------------------------
    107    subroutine mes_precompute()
    108       double precision :: dr
    109       integer :: n, N_
    110       logical :: file_exist
    111 ! collocation pts
    112 ! T_{N-1}(x) has N extrema
    113 ! Numerical Recipes 5.8.5
    114       if(1>i_N) stop '_Nr>i_N'

