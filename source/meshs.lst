    238 !------------------------------------------------------------------------
    239 !  given n pts x, find weights w to get i^th deriv at x0
    240 !------------------------------------------------------------------------
    241    subroutine mes_weights(i,n,x,x0, w)
    242       integer,          intent(in)  :: i, n
    243       double precision, intent(in)  :: x(n), x0
    244       double precision, intent(out) :: w(n)
    245       double precision :: A(n,n)
    246       integer :: j
    247    
    248       A(:,1) = 1d0
    249       do j = 2, n
    250          A(:,j) = A(:,j-1) * (x-x0) / dble(j-1)
    251       end do
    252       call mes_mat_invert(n,A,n)
    253       w = A(i+1,:)
    254 	 
    255    end subroutine mes_weights 


