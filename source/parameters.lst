    102    integer,          parameter :: i_KL  = 4   
    103    integer,          parameter :: i_K1  = i_K-1
    104    integer,          parameter :: i_M1  = i_M-1
    105    integer,          parameter :: i_Z   = 3*i_K  
    106    integer,          parameter :: i_Th  = 3*i_M
    107    integer,          parameter :: i_H1  = (2*i_K1+1)*i_M-i_K1-1
    108    integer,          parameter :: i_pN  = (1+i_N-1)/1
