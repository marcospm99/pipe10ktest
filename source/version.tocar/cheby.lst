     38 !
     39       SUBROUTINE EVAL2 (A, M, X, NX, Y)
     40 !     Last change: 20 Jan 1990
     41 !     Purpose    : to compute the value of the Cheby series
     42 !                  A(1),...A(M) of length M, where A(1)....A(M)
     43 !                  are respectively the coeffs of T(0)...T(M-1),
     44 !                  at the points X(j) j=1,...NX.
     45 !                  The output is Y(j) j=1,...NX, which is the
     46 !                  value of the series at these NX points.
     47       DOUBLE PRECISION A(M),X(NX),Y(NX),B(0:502),XA,XB,XX
     48       INTEGER M,NX,IA,M1,M2,J,I
     49       IA = 502
     50       M1 = M + 1
     51       M2 = M + 2
     52       IF (M2 .GT. IA) GO TO 30
     53       DO 20 I = 1, NX
     54         XX = X(I)
     55         XA = 2D0 * XX - 1D0
     56         XB = 2D0 * XA
     57         B(M2) = 0D0
     58         B(M1) = 0D0
     59         DO 10 J = M - 1, 0, -1
     60           B(J + 1) = XB * B(J + 2) - B(J + 3) + A(J + 1)
     61    10   CONTINUE 
     62         Y(I) = B(1) - XA * B(2)
     63    20 CONTINUE 
     64       RETURN 
     65    30 WRITE (6,40)
     66    40 FORMAT (' M too big in Sub EVAL2')
     67       RETURN 
     68       END 

