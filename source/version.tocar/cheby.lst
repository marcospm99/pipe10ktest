      3       SUBROUTINE CHEBY (M, ND, X, NP, Y)
      4 !     Last change: 20 Jan 1990
      5 !     Purpose    : to find the values Y(1),...Y(NP)
      6 !                  of the NDthe derivative of the Cheby poly
      7 !                  of degree M at the points
      8 !                  X(1),...X(NP)
      9       INTEGER ONE,M,NP,IA,MM,J,ND,NJ 
     10       DOUBLE PRECISION A(500),AX(500),X(NP),Y(NP)
     11       IA = 500
     12       MM = M + 1
     13       ONE = 1
     14       IF (MM .GT. IA) GO TO 70
     15       IF (M .EQ. 0) GO TO 50
     16       DO 10 J = 1, M
     17         A(J) = 0D0
     18    10 CONTINUE 
     19       A(MM) = 1D0
     20       IF (ND .EQ. 0) GO TO 40
     21       DO 30 NJ = 1, ND
     22         CALL DERIV2(A, AX, MM)
     23         DO 20 J = 1, MM
     24           A(J) = AX(J)
     25    20   CONTINUE 
     26    30 CONTINUE 
     27    40 CALL EVAL2(A, MM, X, NP, Y)
     28       RETURN 
     29    50 DO 60 J = 1, NP
     30         IF (ND .EQ. 0) Y(J) = 1D0
     31         IF (ND .GT. 0) Y(J) = 0D0
     32    60 CONTINUE 
     33       RETURN 
     34    70 WRITE (6,80)
     35    80 FORMAT (' Sub CHEBY, M too big')
     36       RETURN 
     37       END 

