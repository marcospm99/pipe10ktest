     74    integer,          parameter :: i_N           = 192 ! n in [1,N] radial r_n
     75    integer,          parameter :: i_K           = 512 ! ! Este tiene que ser potencia de 2 y 3 y grande!!
     76    integer,          parameter :: i_M           = 512 ! azimutal
     77    integer,          parameter :: i_Mp          = 1  ! Siempre 1.
