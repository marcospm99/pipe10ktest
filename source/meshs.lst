    115       mes_D%N = i_N
    116 
    117       inquire(file='mesh.in', exist=file_exist)
    118       if(file_exist) then
    119          if(mpi_rnk==0) print*, ' using custom mesh!'
    120          open(99,status='old',file='mesh.in')
    121          read(99,*) n
    122          if(n/=i_N) stop 'mes_precompute: mesh.in err1: N mismatch'
    123          do n = 1, i_N
    124             read(99,*) mes_D%r(n,1) 
    125          end do
    126          close(99)
    127          if(mes_D%r(1,1)<0d0) stop 'mes_precompute: mesh.in err 2'
    128          dr = dabs(mes_D%r(i_N,1)-1d0)
    129          if(dr>1d-8) stop 'mes_precompute: mesh.in err 3'
    130          
    131       else ! default mesh
    132          N_ = i_N+int(dsqrt(dble(i_N)))
    133          do n = N_-i_N+1, N_
    134             mes_D%r(n-N_+i_N,1) = 0.5d0*( 1d0+dcos(d_PI*(N_-n)/dble(N_)) )
    135          end do
    136          do n = 1, 10
    137             dr = 1.5d0*mes_D%r(1,1)-0.5d0*mes_D%r(2,1)
    138             mes_D%r(:,1) = mes_D%r(:,1)*(1d0+dr) - dr
    139          end do
    140       end if
    141       
    142       call mes_rdom_init(mes_D)
    143 
    144    end subroutine mes_precompute

