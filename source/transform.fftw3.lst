    149    subroutine tra_spec2phys(s, p)
    150       type (spec), intent(in)  :: s
    151       type (phys), intent(out) :: p
    152       integer :: nh, n,m,m_
    153 ! for each r_n ...
    154       do n = 1, mes_D%pN
    155          if(mpi_rnk/1==0) then
    156             X(0:i_K1,   0) = dcmplx(s%Re(0:i_K1,n),s%Im(0:i_K1,n))
    157             X(i_K:2*i_K,0) = 0d0
    158             X(2*i_K+1:, 0) = dcmplx(s%Re(i_K1:1:-1,n),-s%Im(i_K1:1:-1,n))
    159             m_ = 1
    160             nh = 2*i_K-1
    161          else
    162             m_ = 0
    163             nh = i_K1
    164          end if
    165          do m = m_, ((i_M/1)-1)
    166             X(0:i_K1,   m) = dcmplx(s%Re(nh:nh+i_K1,n),s%Im(nh:nh+i_K1,n))
    167             X(i_K:2*i_K,m) = 0d0
    168             X(2*i_K+1:, m) = dcmplx(s%Re(nh-i_K1:nh-1,n),s%Im(nh-i_K1:nh-1,n))
    169             nh = nh + 2*i_K-1
    170          end do
    171          call dfftw_execute(plan_c2cf)
    172 
    173          Xs(:,0:i_M1) = Y
    174 # 139
    175 
    176          Xs(:,i_M:) = 0d0
    177          call dfftw_execute(plan_c2r)
    178          p%Re(:,:,n) = Ys
    179       end do
    180    
    181    end subroutine tra_spec2phys

