     69 !
     70       SUBROUTINE DERIV2 (A, AX, M)
     71 !     Last change: 20 Jan 1990
     72 !     Purpose    :   given the Cheby series A(1),...A(M)
     73 !                    which represents the function
     74 !                    f=Sum(j=1..M) A(j)*T(j-1)
     75 !                    the subroutines computes the coeffs
     76 !                    AX(1).......AX(M) of the function df/dx
     77 !                    Note that AX(M)=0
     78 !
     79       DOUBLE PRECISION A(M),AX(M)
     80       INTEGER          M,J
     81       AX(M) = 0D0
     82       IF (M .LE. 0) GO TO 70
     83       IF (M .GT. 4) GO TO 50
     84       IF (M .EQ. 1) GO TO 10
     85       IF (M .EQ. 2) GO TO 20
     86       IF (M .EQ. 3) GO TO 30
     87       IF (M .EQ. 4) GO TO 40
     88    10 AX(1) = 0D0
     89       RETURN 
     90    20 AX(1) = 2D0 * A(2)
     91       RETURN 
     92    30 AX(1) = 2D0 * A(2)
     93       AX(2) = 8D0 * A(3)
     94       RETURN 
     95    40 AX(1) = 2D0 * A(2) + 6D0 * A(4)
     96       AX(2) = 8D0 * A(3)
     97       AX(3) = 12D0 * A(4)
     98       RETURN 
     99    50 AX(M - 1) = - 4D0 * (1D0 - M) * A(M)
    100       AX(M - 2) = - 4D0 * (2D0 - M) * A(M - 1)
    101       DO 60 J = M - 3, 2, -1
    102         AX(J) = 4D0 * J * A(J + 1) + AX(J + 2)
    103    60 CONTINUE 
    104       AX(1) = 2D0 * A(2) + AX(3) / 2D0
    105       RETURN 
    106    70 WRITE (6,80)
    107    80 FORMAT (' M .LE. 0 in Sub DERIV2')
    108       RETURN 
    109       END 

