    184 !------------------------------------------------------------------------
    185 !  Convert real to spectral space
    186 !------------------------------------------------------------------------
    187    subroutine tra_phys2spec(p, s)
    188       type (phys), intent (in)  :: p
    189       type (spec), intent (out) :: s
    190       integer :: nh, n,m, m_
    191       double precision :: scale_
    192 ! scale, FFTW 4.7.2
    193       scale_ = 1d0 / dble(i_3K*i_3M)
    194 
    195       do n = 1, mes_D%pN
    196          Ys = scale_ * p%Re(:,:,n)
    197          call dfftw_execute(plan_r2c)
    198 
    199          Y = Xs(:,0:i_M1)
    200 # 170
    201 
    202          call dfftw_execute(plan_c2cb)
    203          if(mpi_rnk/1==0) then
    204             s%Re(0:i_K1,n) =  dble(X(0:i_K1,0))
    205             s%Im(0:i_K1,n) = dimag(X(0:i_K1,0))
    206             m_ = 1
    207             nh = 2*i_K-1
    208          else
    209             m_ = 0
    210             nh = i_K1
    211          end if
    212          do m = m_, ((i_M/1)-1)
    213             s%Re(nh:nh+i_K1,n) =  dble(X(0:i_K1,m))
    214             s%Im(nh:nh+i_K1,n) = dimag(X(0:i_K1,m))
    215             s%Re(nh-i_K1:nh-1,n) =  dble(X(2*i_K+1:,m))
    216             s%Im(nh-i_K1:nh-1,n) = dimag(X(2*i_K+1:,m))
    217             nh = nh + 2*i_K-1
    218          end do
