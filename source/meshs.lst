    147 !------------------------------------------------------------------------
    148 ! for the radial domain D, set the number of points D%N and their
    149 ! values D%r, then mes_rdom_init() will fill in the rest...
    150 !------------------------------------------------------------------------
    151    subroutine mes_rdom_init(D)
    152       type (rdom), intent(inout) :: D
    153       integer, parameter :: lda = 2*i_KL+1
    154       double precision   :: A(lda,lda), w(lda), c,e
    155       double precision   :: r_(1-i_KL:D%N)
    156       integer :: n,i,j,nn, l,r
    157 ! distribute radial points over procs
    158       D%pN_ = 0
    159       do n = 1, D%N
    160          r = 1 - modulo(n-1,1) - 1
    161          D%pN_(r) = D%pN_(r) + 1
    162       end do
    163       D%pNi_(0) = 1
    164       do r = 1, 1-1
    165          D%pNi_(r) = D%pNi_(r-1) + D%pN_(r-1)
    166       end do
    167       do r = 1, (1*1)-1
    168          D%pN_ (r) = D%pN_ (r-1)
    169          D%pNi_(r) = D%pNi_(r-1)
    170       end do
    171       D%pN  = D%pN_ (mpi_rnk)
    172       D%pNi = D%pNi_(mpi_rnk)
    173 ! get ad_r(:,j) = r^j
    174       do j = -3, 3
    175          if(j==1) cycle
    176          D%r(:,j) = D%r(:,1)**j
    177       end do
    178 
    179 ! get finite difference weights
    180       if(i_KL<2) stop 'mes: KL<2'
    181       do i = 1, i_KL
    182          D%dr(i)%M = 0d0
    183       end do
    184       D%radLap%M = 0d0
    185       D%intrdr = 0d0
    186 
    187       r_(1:D%N) =  D%r(:,1)
    188       r_(:0)    = -D%r(i_KL:1:-1,1)
    189 
    190       do n = 1, D%N
    191          l = max(1-i_KL,n-i_KL)
    192          r = min(n+i_KL,D%N)
    193          nn = r-l+1
    194 	 do i = 1, i_KL
    195    	    call mes_weights(i,nn,r_(l),r_(n), w)
    196             do j = l, r
    197                D%dr(i)%M(i_KL+1+n-j,j) = w(j-l+1)
    198             end do
    199 	 end do
    200       end do
    201 ! weights for extrapolation to r=0, 1

Page 6           Source Listing                  MES_RDOM_INIT
2023-01-27 11:38                                 ./program/meshs.f90

    202       do i = 0, i_KL
    203          call mes_weights(i,1+i_KL,r_(1),       0d0, D%dr0(1,i))
    204          call mes_weights(i,1+i_KL,r_(D%N-i_KL),1d0, D%dr1(1,i))
    205       end do
    206 ! get  (1/r)dr + drr
    207       do j = 1-i_KL, D%N
    208          l = max(1,j-i_KL)
    209          r = min(j+i_KL,D%N)
    210          do n = l, r
    211             D%radLap%M(i_KL+1+n-j,j)  &
    212                = D%dr(2)%M(i_KL+1+n-j,j)  &
    213                + D%dr(1)%M(i_KL+1+n-j,j) / r_(n)
    214          end do
    215       end do
    216 ! weights for integration  r dr
    217       do n = 1, D%N
    218          l = max(1,n-i_KL)
    219          r = min(n+i_KL,D%N)
    220          nn = r-l+1
    221 	 do i = 1, nn
    222 	    call mes_weights(i-1,nn,r_(l),r_(n), w)
    223 	    A(i,:) = w 
    224 	 end do
    225          c = 1d0
    226          e = 1d0
    227          do i = 1, nn
    228             c = c * (r_(min(n+1,D%N))-r_(n)) / dble(i)
    229             e = e * (r_(max(  0,n-1))-r_(n)) / dble(i)
    230             D%intrdr(l:l+nn-1) =  &
    231                D%intrdr(l:l+nn-1) + 0.5d0*(c-e)*A(i,1:nn)*r_(n)
    232          end do
    233       end do
    234 
    235    end subroutine mes_rdom_init

