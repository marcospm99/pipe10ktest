     83    logical,          parameter :: b_mirrorsym   = .false.
     84    logical,          parameter :: b_shiftrefl   = .false.
     85    logical,          parameter :: b_shiftrott   = .false.
     86    double precision, parameter :: d_minE3d      = 1d-5
     87    
     88 !-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
     89    integer,          parameter :: i_save_rate1  = 2000!100
     90    integer,          parameter :: i_save_rate2  = 10  
     91    integer,          parameter :: i_maxtstep    = -1 
     92    double precision, parameter :: d_maxt        = -1d0
     93    double precision, parameter :: d_cpuhours    = 1d99 !90d0
     94    double precision, parameter :: d_time        = 0d0 !-1d0 !0d0
     95    double precision, parameter :: d_timestep    = 0.01d0 !-1d0
     96    double precision, parameter :: d_maxdt       = 1d99
     97    double precision, parameter :: d_dterr       = 1d-5 !1d99
     98    double precision, parameter :: d_courant     = 0.5d0
