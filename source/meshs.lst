    258 !------------------------------------------------------------------------
    259 !  Replace nxn matrix A by its inverse
    260 !------------------------------------------------------------------------
    261    subroutine mes_mat_invert(n,A,lda)
    262       integer, intent(in) :: n, lda
    263       double precision, intent(inout) :: A(lda,n)
    264       double precision :: work(n)
    265       integer :: info, ipiv(n)
    266    
    267       call dgetrf(n, n, A, lda, ipiv, info)
    268       if(info /= 0) stop 'matrix inversion error 1'
    269 
    270       call dgetri(n, A, lda, ipiv, work, n, info)
    271       if(info /= 0) stop 'matrix inversion error 2'
    272 
    273    end subroutine mes_mat_invert


