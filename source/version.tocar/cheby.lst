    110 !
    111       SUBROUTINE CMESH (N, X, NKIND)
    112 !     Last changed: 20 Jan 1990
    113 !     Purpose :     Chebyshev mesh:
    114 !                   it finds N points X(j) j=1,...N which are the
    115 !                   zeroes of the Cheby polynomial of first kind Tn
    116 !                   or of the Cheby polynomial of second kind Un,
    117 !                   over the interval 0 < x < 1
    118       DOUBLE PRECISION X(N),PI,y
    119       INTEGER N,IV,I,NKIND
    120       PI = 4D0 * DATAN (1D0)
    121       IF (NKIND .EQ. 1) GO TO 10
    122       IF (NKIND .EQ. 2) GO TO 30
    123 !     Zeroes of the Chebyshev polynomials of the first kind
    124    10 DO 20 I = 1, N
    125         IV = N + 1 - I
    126         X(I) = 0.5D0 * (1D0+DCOS((2*IV - 1)*PI/(2D0*N)))
    127    20 CONTINUE 
    128       RETURN 
    129 !     Zeroes of the Chebyshev polynomials of the second kind
    130    30 DO 40 I = 1, N
    131         Y = DCOS(PI*I/(N + 1))
    132         X(I) = 0.5D0 * (Y + 1D0)
    133    40 CONTINUE 
    134       RETURN 
    135       END 


